{
    "name": "rati",
    "version": "0.2.0-alpha.12",
    "description": "TypeScript frontend framework for React & MobX",
    "author": "Nazar Chobaniuk",
    "license": "MIT",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "dist/main.js",
    "types": "dist/main.d.ts",
    "exports": {
        ".": {
            "types": "./dist/main.d.ts",
            "import": "./dist/main.js",
            "source": "./src/main.ts"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "clean": "rm -fr dist",
        "build": "yarn clean && yarn -T run tsc",
        "watch": "yarn -T run tsc --watch",
        "test": "yarn tsd"
    },
    "dependencies": {
        "@sindresorhus/is": "^5.3.0",
        "type-fest": "^2.19.0"
    },
    "devDependencies": {
        "@types/lodash": "~4.14.171",
        "@types/react": "^18.2.79",
        "history": "^5.0.0",
        "lodash": "~4.17.21",
        "mobx": "^6.12.3",
        "mobx-react-lite": "^4.0.7",
        "react": "^18.2.0",
        "tsd": "^0.23.0"
    },
    "peerDependencies": {
        "history": "^5.0.0",
        "lodash": "~4.17.21",
        "mobx": "^6.3.10",
        "mobx-react-lite": "^4.0.7",
        "react": "^18.2.0"
    },
    "tsd": {
        "directory": "src/test-d"
    }
}
